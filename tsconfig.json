{
	"compilerOptions": {
		"lib": [
			"dom",
			"esnext"
		],
		"module": "es6",
		"moduleResolution": "node",
		"target": "es2017",
		"sourceMap": true,
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"pretty": true
	},
	"awesomeTypescriptLoaderOptions": {
		"inlineSourceMap": true,
		"sourceMap": false,
		"noEmitOnError": true
	}
}
